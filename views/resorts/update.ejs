<%- include('../partials/header') %>
<p>
  Your upcoming trip to
  <a href="/resorts/<%= resort._id%>/resortName %>"><%=resort.resortName%></a>
</p>
<form
  id="update"
  action="/resorts/<%= resort._id%>/<%=updateField%>?_method=PUT"
  method="POST"
>
  <label><%= updateField %>:</label>
  <input type="text" name="<%=updateField%>" />
  <button type="submit">Submit Destination</button>
</form>

<%- include('../partials/footer') %>
